<UserControl x:Class="AppDesk.UserControls.Campos.Masked.RenavamFieldUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AppDesk.UserControls.Campos.Masked"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             xmlns:s="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d">
    <StackPanel Style="{StaticResource MainStackPanelStyle}" Grid.Row="1" Grid.ColumnSpan="2">
        <Label Style="{StaticResource MainLabelRoundStyle}">
            RENAVAM
        </Label>
        <xctk:MaskedTextBox x:Name="RenavamTextBox" TextChanged="RenavamTextBox_TextChanged" Style="{StaticResource MainTextBoxStyle}" Width="100" ValueDataType="{x:Type s:String}" Mask="99999999999">
            <TextBox.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="0 5 5 0"/>
                </Style>
            </TextBox.Resources>
        </xctk:MaskedTextBox>
    </StackPanel>
</UserControl>
