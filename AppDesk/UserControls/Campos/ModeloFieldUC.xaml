<UserControl x:Class="AppDesk.UserControls.Campos.ModeloFieldUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AppDesk.UserControls.Campos"
             mc:Ignorable="d">
    <StackPanel Style="{StaticResource MainStackPanelStyle}">
        <Label Style="{StaticResource MainLabelRoundStyle}">
            MODELO
        </Label>
        <TextBox x:Name="ModeloTextbox" Style="{StaticResource MainTextBoxStyle}" Width="100" >
            <TextBox.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="0 5 5 0"/>
                </Style>
            </TextBox.Resources>
        </TextBox>
    </StackPanel>
</UserControl>
